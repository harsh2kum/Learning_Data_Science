[Unit]
Description=Gunicorn daemon for Django project
Requires=gunicorn.socket

After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/python-backend/backend
ExecStart=/home/ubuntu/python-backend/backend/.venv/bin/gunicorn --workers 10 --bind 0.0.0.0:9080 --preload --timeout 95 core.wsgi:application



[Install]
WantedBy=multi-user.target